          },
          {
            "name": "SM",
            "code": "<DatePicker label=\"Small (SM)\" placeholder=\"32px height\" size=\"sm\" />"
          },
          {
            "name": "XL",
            "code": "<DatePicker label=\"Extra Large (XL)\" placeholder=\"56px height\" size=\"xl\" />"
          },
          {
            "name": "XS",
            "code": "<DatePicker label=\"Extra Small (XS)\" placeholder=\"24px height\" size=\"xs\" />"
          },
          {
            "name": "XXL",
            "code": "<DatePicker label=\"2X Large (XXL)\" placeholder=\"64px height\" size=\"xxl\" />"
          }
        ]
