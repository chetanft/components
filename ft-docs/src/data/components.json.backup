{
  "designSystem": {
    "components": {
      "Dropdown": {
        "description": "A dropdown select component with search functionality, segmented tabs support, and various sizes and states.",
        "import": "import { Dropdown } from '@chetanft/design_system';",
        "props": {
          "className": {
            "type": "string",
            "required": false,
            "description": "Additional CSS classes"
          },
          "label": {
            "type": "string",
            "required": false,
            "description": "Label text for the dropdown"
          },
          "placeholder": {
            "type": "string",
            "required": false,
            "default": "Select an option",
            "description": "Placeholder text"
          },
          "type": {
            "type": "union of 'default', 'search'",
            "required": false,
            "default": "default",
            "description": "Type of dropdown - default or search"
          },
          "size": {
            "type": "union of 'xs', 'sm', 'md', 'lg', 'xl', 'xxl'",
            "required": false,
            "default": "md",
            "description": "Size of the dropdown"
          },
          "state": {
            "type": "union of 'default', 'error', 'disabled'",
            "required": false,
            "default": "default",
            "description": "Visual state of the dropdown"
          },
          "options": {
            "type": "array",
            "required": true,
            "description": "Array of options with value and label properties"
          },
          "segments": {
            "type": "array",
            "required": false,
            "description": "Array of segment tabs for filtering (used with search type)"
          },
          "selectedSegment": {
            "type": "string",
            "required": false,
            "description": "Currently selected segment value"
          },
          "error": {
            "type": "string",
            "required": false,
            "description": "Error message to display"
          }
        },
        "examples": [
          {
            "name": "Default",
            "code": "<Dropdown label=\"Default\" state=\"default\" placeholder=\"Default state\" options={[ { value: 'option1', label: 'Option 1' }, { value: 'option2', label: 'Option 2' }, ]} />"
          },
          {
            "name": "Error",
            "code": "<Dropdown label=\"Error\" state=\"error\" error=\"This field has an error\" placeholder=\"Error state\" options={[ { value: 'option1', label: 'Option 1' }, { value: 'option2', label: 'Option 2' }, ]} />"
          },
          {
            "name": "Search",
            "code": "<Dropdown label='Search Dropdown' type='search' placeholder='Search options' />"
          }
        ]
      },
      "Tooltip": {
        "description": "A tooltip component for displaying contextual information with various placements and styles.",
        "import": "import { Tooltip } from '@chetanft/design_system';",
        "props": {
          "className": {
            "type": "string",
            "required": false,
            "description": "Additional CSS classes"
          },
          "children": {
            "type": "ReactNode",
            "required": true,
            "description": "Tooltip content text"
          },
          "heading": {
            "type": "string",
            "required": false,
            "description": "Optional heading text for the tooltip"
          },
          "placement": {
            "type": "union of 'top', 'bottom', 'left', 'right'",
            "required": false,
            "default": "top",
            "description": "Placement of the tooltip relative to trigger"
          },
          "align": {
            "type": "union of 'start', 'center', 'end'",
            "required": false,
            "default": "center",
            "description": "Alignment of the tooltip"
          },
          "color": {
            "type": "union of 'white', 'dark'",
            "required": false,
            "default": "white",
            "description": "Color theme of the tooltip"
          },
          "showClose": {
            "type": "boolean",
            "required": false,
            "default": false,
            "description": "Whether to show close button"
          },
          "primaryActionText": {
            "type": "string",
            "required": false,
            "description": "Text for primary action button"
          },
          "secondaryActionText": {
            "type": "string",
            "required": false,
            "description": "Text for secondary action button"
          }
        },
        "examples": [
          {
            "name": "Default",
            "code": "<Tooltip placement='top' align='center' color='white'>This is a basic tooltip</Tooltip>"
          }
        ]
      },
      "ButtonGroup": {
        "description": "A group of buttons displayed together with consistent spacing and styling.",
        "import": "import { ButtonGroup } from '@chetanft/design_system';",
        "props": {
          "className": {
            "type": "string",
            "required": false,
            "description": "Additional CSS classes"
          },
          "buttons": {
            "type": "array",
            "required": true,
            "description": "Array of button configurations with id, label, variant, icon, and iconPosition"
          },
          "equalWidth": {
            "type": "boolean",
            "required": false,
            "default": false,
            "description": "Whether all buttons should have equal width"
          }
        },
        "examples": [
          {
            "name": "Primary",
            "code": "<ButtonGroup id='ghost' label='Button' variant='text' icon='add' iconPosition='leading' id='outline' label='Button' variant='secondary' icon='add' iconPosition='leading' id='filled' label='Button' variant='primary' icon='add' iconPosition='leading' />"
          }
        ]
      },
      "DatePicker": {
        "description": "A date picker component for selecting single dates or date ranges with various sizes and states.",
        "import": "import { DatePicker } from '@chetanft/design_system';",
        "props": {
          "className": {
            "type": "string",
            "required": false,
            "description": "Additional CSS classes"
          },
          "label": {
            "type": "string",
            "required": false,
            "description": "Label text for the date picker"
          },
          "placeholder": {
            "type": "string",
            "required": false,
            "default": "MM/DD/YYYY",
            "description": "Placeholder text"
          },
          "size": {
            "type": "union of 'xs', 'sm', 'md', 'lg', 'xl', 'xxl'",
            "required": false,
            "default": "md",
            "description": "Size of the date picker"
          },
          "value": {
            "type": "string",
            "required": false,
            "description": "Selected date value"
          },
          "range": {
            "type": "boolean",
            "required": false,
            "default": false,
            "description": "Whether to enable date range selection"
          },
          "startValue": {
            "type": "string",
            "required": false,
            "description": "Start date value (for range mode)"
          },
          "endValue": {
            "type": "string",
            "required": false,
            "description": "End date value (for range mode)"
          },
          "disabled": {
            "type": "boolean",
            "required": false,
            "default": false,
            "description": "Whether the date picker is disabled"
          },
          "error": {
            "type": "boolean",
            "required": false,
            "default": false,
            "description": "Whether the date picker is in error state"
          },
          "onChange": {
            "type": "function",
            "required": false,
            "description": "Callback when date value changes"
          },
          "onStartChange": {
            "type": "function",
            "required": false,
            "description": "Callback when start date changes (for range mode)"
          },
          "onEndChange": {
            "type": "function",
            "required": false,
            "description": "Callback when end date changes (for range mode)"
          }
        },
        "examples": [
          {
            "name": "Default",
            "code": "<DatePicker label='Date' placeholder='MM/DD/YYYY' value=\"new Date().toLocaleDateString()\" />"
          },
          {
            "name": "Error",
            "code": "<DatePicker label='Date with Error' placeholder='MM/DD/YYYY' error />"
          },
          {
            "name": "Disabled",
            "code": "<DatePicker label='Disabled Date' placeholder='MM/DD/YYYY' disabled />"
          },
          {
            "name": "LG",
